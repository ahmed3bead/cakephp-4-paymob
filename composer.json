{
    "name": "ahmed3bead/cakephp-4-paymob",
    "description": "Cakephp Paymob plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "~4.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "CakephpPaymob\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakephpPaymob\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}